<%- contentFor('hero') %>
<section class="hero">
    <div class="hero-content">
        <h2> All about us...</h2>
        <p>"Lorem ipsum dolor sit amet, consectetur adipiscing elit. Mauris nec dui magna."</p>
        <a href="/contact" class="hvr-buzz btn btn-lg btn-primary">Contact</a>
    </div>
</section>
<%- contentFor('body') %>
<div class="jumbotron text-center">
    <h1>The Team</h1>

    <div class="row">
    <% for (user of users) { %> 
    	<div class="col-sm-5">
    	<h2><%= user.name %></h2>
    	<h3><%= user.email %></h3>
    <img src="<%= user.avatar %>" alt="" class="img-responsive">
   
    </div> 
    <% } %>
</div>
</div>